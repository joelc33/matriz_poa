	<queryString>
		<![CDATA[SELECT t46.id_ejercicio as ejercicio, t46.id_ejecutor as ej_id, t24.tx_ejecutor as ej_nombre,
	sp_ejecutor_ac_monto( t46.id_ejercicio::text, t46.id_ejecutor ) as ej_monto,
	sp_ejecutor_ac_monto_calc( t46.id_ejercicio::text, t46.id_ejecutor ) as ej_monto_calc,
		(select count(*) as ac_registradas from t46_acciones_centralizadas WHERE id_ejercicio = t46.id_ejercicio and id_ejecutor = t46.id_ejecutor) as ac_registradas,
	t46.id_accion as ac_id,
	de_nombre as ac_nombre, t53.numero as ae_id, t53.nombre as ae_nombre, t46.monto as ac_monto, coalesce(t46.monto_calc, 0) as ac_monto_calc,
	(select count(*) as ac_max from mantenimiento.tab_ac_predefinida) as ac_max,
	 (select count(t53.nu_numero) as ae_max
			from mantenimiento.tab_ac_predefinida as t52
				inner join mantenimiento.tab_ac_ae_predefinida as t53 on t53.id_padre = t52.id
				where t52.id = t46.id_accion
		) as ae_max,
	(select t46.id is not null as ac_existe
		from t46_acciones_centralizadas as t46i	where
		t46i.id_ejercicio = t46.id_ejercicio and t46i.id_ejecutor = t46.id_ejecutor and t46i.id_accion = t46.id_accion

	) as ac_existe,
	(select count(*) as ae_registradas from t47_ac_accion_especifica WHERE id_accion_centralizada = t46.id) as ae_registradas,
	t47.monto as ae_monto,
	t47.monto_calc as ae_monto_calc,
	(select count(*) as mt_registradas from t69_metas_ac WHERE id_accion_centralizada = t69.id_accion_centralizada and co_ac_acc_espec = t69.co_ac_acc_espec) as mt_registradas,
	codigo as mt_codigo,
	(case when char_length(t69.nb_meta) < 180 then t69.nb_meta else substring(t69.nb_meta from 1 for 177) || '...' end) as mt_nombre,
	monto_cargado_ac_ae_meta(co_metas) AS mt_monto,
	(select sum(monto) as ac_ae_monto from t47_ac_accion_especifica WHERE id_accion_centralizada = t46.id and edo_reg is true) as ac_ae_monto,
	(select sum(monto) as ac_ae_monto_calc from t54_ac_ae_partidas WHERE id_accion_centralizada = t47.id_accion_centralizada and edo_reg is true) as ac_ae_monto_calc,
	(select sum(monto) as ef_monto from t46_acciones_centralizadas WHERE id_ejercicio = t46.id_ejercicio and edo_reg is true) as ef_monto,
	(select sum(mo_presupuesto) as ef_monto_calc from t70_metas_ac_detalle as t700
	inner join t69_metas_ac as t699 on t699.co_metas = t700.co_metas
	inner join t47_ac_accion_especifica as t477 on t477.id_accion_centralizada = t699.id_accion_centralizada and t477.id_accion = t699.co_ac_acc_espec
	inner join t46_acciones_centralizadas as t466 on t466.id = t477.id_accion_centralizada
	WHERE id_ejercicio = t46.id_ejercicio and t466.edo_reg is true and t477.edo_reg is true and t699.edo_reg is true and t700.edo_reg is true) as ef_monto_calc
	FROM t69_metas_ac as t69
	inner join t47_ac_accion_especifica as t47 on t47.id_accion_centralizada = t69.id_accion_centralizada and t47.id_accion = t69.co_ac_acc_espec
	inner join t53_ac_ae_predefinidas as t53 on t53.id = t47.id_accion
	inner join t46_acciones_centralizadas as t46 on t46.id = t47.id_accion_centralizada
	inner join mantenimiento.tab_ac_predefinida as t52 on t52.id = t46.id_accion
	inner join mantenimiento.tab_ejecutores as t24 on t46.id_ejecutor = t24.id_ejecutor
	WHERE t46.id_ejercicio::text = $P{id_ejercicio}
	and t69.edo_reg is true
	order by t46.id_ejecutor, t46.id_accion, t53.numero, codigo ASC]]>
	</queryString>
