	<parameter name="id_ejercicio" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT tf.co_tipo_recurso, tr.tx_tipo_recurso, pr.id_proyecto, pr.nombre, sum( di.mo_fondo ) as monto
  FROM t26_proyectos pr
  LEFT JOIN t62_proyecto_distribucion di ON di.id_proyecto = pr.id_proyecto  AND di.edo_reg
  LEFT JOIN t61_tipo_fondo tf ON tf.co_tipo_fondo = di.co_tipo_fondo AND tf.edo_reg
  LEFT JOIN t60_tipo_recurso tr ON tr.co_tipo_recurso = tf.co_tipo_recurso  AND tr.edo_reg
  WHERE pr.id_ejercicio=$P{id_ejercicio} AND pr.edo_reg AND tf.co_tipo_recurso=$P{id_tipo_recurso} AND di.mo_fondo>0 AND di.edo_reg is true
  GROUP BY tf.co_tipo_recurso, tr.tx_tipo_recurso, pr.id_proyecto, pr.nombre
  ORDER BY co_tipo_recurso, pr.id_proyecto;]]>
	</queryString>
