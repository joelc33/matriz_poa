	<queryString>
		<![CDATA[SELECT vi.id_obj_historico, ob.tx_descripcion as descripcion, pr.id_proyecto, pr.nombre, pr.objetivo, pr.monto
  FROM t26_proyectos pr
  INNER JOIN t32_proyecto_vinculos vi ON vi.id_proyecto = pr.id_proyecto AND vi.edo_reg
  INNER JOIN t20_planes ob ON ob.co_objetivo_historico= vi.id_obj_historico AND ob.nu_nivel=1 AND ob.co_objetivo_historico = $P{ID_OBJ} AND ob.edo_reg
WHERE pr.edo_reg AND pr.id_ejercicio=$P{id_ejercicio}
  ORDER BY id_obj_historico,id_proyecto]]>
	</queryString>
