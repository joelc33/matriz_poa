	<queryString>
		<![CDATA[SELECT distinct vi.id_obj_historico, ob.tx_descripcion AS descripcion, sum(pr.monto) as sum_monto
  FROM t26_proyectos pr
  INNER JOIN t32_proyecto_vinculos vi ON vi.id_proyecto = pr.id_proyecto AND vi.edo_reg IS true
  LEFT JOIN t20_planes ob ON ob.co_objetivo_historico= vi.id_obj_historico AND ob.nu_nivel=1 and ob.edo_reg IS true
  WHERE pr.edo_reg AND pr.id_ejercicio=$P{id_ejercicio}
  GROUP by vi.id_obj_historico  , ob.tx_descripcion
  ORDER BY id_obj_historico;]]>
	</queryString>
