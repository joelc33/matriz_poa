	<parameter name="id_proyecto" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
DISTINCT di.id_proyecto,
SUM(CASE WHEN di.co_tipo_fondo=1 THEN di.mo_fondo ELSE 0 END) AS f1,
SUM(CASE WHEN di.co_tipo_fondo=2 THEN di.mo_fondo ELSE 0 END) AS f2,
SUM(CASE WHEN di.co_tipo_fondo=3 THEN di.mo_fondo ELSE 0 END) AS f3,
SUM(CASE WHEN di.co_tipo_fondo=4 THEN di.mo_fondo ELSE 0 END) AS f4,
SUM(CASE WHEN di.co_tipo_fondo>4 THEN di.mo_fondo ELSE 0 END) AS f5,
SUM(di.mo_fondo) AS ft
FROM t62_proyecto_distribucion di
WHERE di.id_proyecto=$P{id_proyecto} AND di.edo_reg
GROUP BY di.id_proyecto]]>
	</queryString>
	<field name="id_proyecto" class="java.lang.String"/>
