	</parameter>
	<queryString>
		<![CDATA[SELECT
pr.id_proyecto as id_proyecto,
ae.tx_codigo, substr(ae.descripcion,1,168) AS accionespc, ae.total
FROM
t26_proyectos pr
LEFT JOIN t39_proyecto_acc_espec ae ON ae.id_proyecto =  pr.id_proyecto AND ae.edo_reg
WHERE
pr.edo_reg AND pr.id_proyecto=$P{id_proyecto}
AND id_padre is null
ORDER BY tx_codigo;]]>
	</queryString>
