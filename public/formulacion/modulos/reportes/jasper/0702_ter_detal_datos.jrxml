	</parameter>
	<queryString>
		<![CDATA[SELECT  distinct mu.co_estado, mu.co_municipio, mu.tx_municipio, pr.id_proyecto, pr.nombre
  FROM t13_municipio mu
  LEFT JOIN t34_proyecto_localizacion_nacional ln ON  ln.co_municipio=mu.co_municipio AND ln.edo_reg
  INNER JOIN t26_proyectos pr ON  pr.id_proyecto=ln.id_proyecto  AND pr.id_ejercicio = $P{id_ejercicio}
  AND pr.edo_reg
  where mu.co_estado=23 AND mu.co_municipio=$P{co_municipio}
  order by mu.co_municipio, pr.id_proyecto;]]>
	</queryString>
