	<queryString>
		<![CDATA[SELECT DISTINCT pr.clase_sector, sc.tx_descripcion as descripcion, sum(pr.monto) AS sum_monto
  FROM t26_proyectos pr
  LEFT JOIN t18_sectores sc ON sc.co_sector = pr.clase_sector AND sc.nu_nivel =1 AND sc.edo_reg
  WHERE pr.id_ejercicio=$P{id_ejercicio}
  AND pr.edo_reg
  GROUP BY pr.clase_sector, sc.tx_descripcion
  ORDER BY clase_sector;]]>
	</queryString>
